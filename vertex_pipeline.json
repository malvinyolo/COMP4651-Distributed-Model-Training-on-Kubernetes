                "location": {
                  "componentInputParameter": "pipelinechannel--region"
                },
                "pipelinechannel--accelerator_count": {
                  "componentInputParameter": "pipelinechannel--accelerator_count"
                },
                "pipelinechannel--accelerator_type": {
                  "componentInputParameter": "pipelinechannel--accelerator_type"
                },
                "pipelinechannel--artifact_registry_image_prefix": {
                  "componentInputParameter": "pipelinechannel--artifact_registry_image_prefix"
                },
                "pipelinechannel--gcs_output_prefix": {
                  "componentInputParameter": "pipelinechannel--gcs_output_prefix"
                },
                "pipelinechannel--gpus_per_node": {
                  "componentInputParameter": "pipelinechannel--gpus_per_node"
                },
                "pipelinechannel--machine_type": {
                  "componentInputParameter": "pipelinechannel--machine_type"
                },
                "pipelinechannel--num_nodes": {
                  "componentInputParameter": "pipelinechannel--num_nodes"
                },
                "pipelinechannel--train_image": {
                  "componentInputParameter": "pipelinechannel--train_image"
                },
                "project": {
                  "componentInputParameter": "pipelinechannel--project_id"
                },
                "worker_pool_specs": {
                  "runtimeValue": {
                    "constant": [
                      {
                        "container_spec": {
                          "command": [
                            "python",
                            "entrypoint_train.py",
                            "--distributed",
                            "--gpus_per_node",
                            "{{$.inputs.parameters['pipelinechannel--gpus_per_node']}}",
                            "--args",
                            "--data_gcs_prefix",
                            "{{$.inputs.parameters['pipelinechannel--gcs_output_prefix']}}"
                          ],
                          "image_uri": "{{$.inputs.parameters['pipelinechannel--artifact_registry_image_prefix']}}/{{$.inputs.parameters['pipelinechannel--train_image']}}"
                        },
                        "machine_spec": {
                          "accelerator_count": "{{$.inputs.parameters['pipelinechannel--accelerator_count']}}",
                          "accelerator_type": "{{$.inputs.parameters['pipelinechannel--accelerator_type']}}",
                          "machine_type": "{{$.inputs.parameters['pipelinechannel--machine_type']}}"
                        },
                        "replica_count": "{{$.inputs.parameters['pipelinechannel--num_nodes']}}"
                      }
                    ]
                  }
                }
              }
            },
            "taskInfo": {
              "name": "custom-training-job-2"
            }
          }
        }
      },
